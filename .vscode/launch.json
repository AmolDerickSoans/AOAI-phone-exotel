{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: App",
            "type": "debugpy",
            "request": "launch",
            "module": "app.IVR.exotel",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "python": "${workspaceFolder}/venv/bin/python",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "justMyCode": false
        }
    ]
}